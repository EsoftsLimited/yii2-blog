    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "trippo/responsivefilemanager",
                "version": "dev-master",
                "source": {
                    "type": "git",
                    "url": "https://github.com/trippo/responsivefilemanager",
                    "reference": "master"
                }
            }
        }
    ]
